#!/usr/bin/env zsh
################################################################################
#    Author: Wenxuan Zhang                                                     #
#     Email: wenxuangm@gmail.com                                               #
#   Created: 2020-01-03 16:33                                                  #
################################################################################
# zmodload zsh/zprof

# Put this line on top to make keybinds work
bindkey -e

source ~/.zsh_env

[ -f ~/.fzf.zsh       ] && source ~/.fzf.zsh
[ -f ~/.zsh_fzf_extra ] && source ~/.zsh_fzf_extra
[ -f ~/.zsh_local     ] && source ~/.zsh_local
[ -f ~/.zsh_secret    ] && source ~/.zsh_secret

source ~/.zsh_plug
source ~/.zsh_keybinds
source ~/.zsh_misc
source ~/.zsh_aliases
source ~/.zsh_custom

[ -f ~/.zsh_theme ] && source ~/.zsh_theme

[ -f ~/.travis/travis.sh ] && source ~/.travis/travis.sh

# Theme
(( $+commands[starship] )) && source <(starship init zsh)

return 0
