{
  "python.jediEnabled": true,
  "python.pythonPath": "~/.pyenv/shims/python",
  "diagnostic.hintSign": "──",
  "diagnostic.infoSign": "──",
  "diagnostic.warningSign": "──",
  "diagnostic.errorSign": "══",
  "languageserver": {
    "clangd": {
      "command": "clangd",
      "args": ["--background-index"],
      "rootPatterns": ["compile_flags.txt", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "filetypes": ["c", "cpp", "objc", "objcpp"]
    }
  },
  "coc.preferences.formatOnSaveFiletypes": ["rust"],
  "coc.preferences.colorSupport": false,
  "coc.source.emoji.triggerCharacters": [":"],
  "coc.source.emoji.filetypes": ["markdown", "gitcommit"],
  "coc.source.file.ignoreHidden": false,
  "coc.source.around.firstMatch": false,
  "coc.source.buffer.firstMatch": false,
  "coc.source.syntax.firstMatch": false,
  "coc.preferences.extensionUpdateCheck": "daily",
  "tabnine.priority": 99,
  "suggest.languageSourcePriority": 100,
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "gosnippets/UltiSnips"
  ],
  "snippets.userSnippetsDirectory": "~/.vim/UltiSnips",
  "suggest.completionItemKindLabels": {
    "function": "\uf794",
    "method": "\uf6a6",
    "variable": "\uf71b",
    "constant": "\uf8ff",
    "struct": "\ufb44",
    "class": "\uf0e8",
    /* "interface": "\ufa52", */
    "interface": "",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf668",
    "color": "\ue22b",
    "property": "\ufab6",
    /* "field": "\uf93d", */
    "field": "",
    "unit": "\uf475",
    "file": "\uf471",
    "value": "\uf8a3",
    "event": "\ufacd",
    "folder": "\uf115",
    "keyword": "\uf893",
    "snippet": "\uf64d",
    "operator": "\uf915",
    "reference": "\uf87a",
    "typeParameter": "\uf278",
    "default": "\uf29c",
  }
}
