[preferences]
button-layout='appmenu:minimize,maximize,close'
num-workspaces=2
workspace-names=['Work', 'Not work']

[keybindings]
minimize=@as []
move-to-monitor-right=['<Super>l']
maximize=['<Shift><Alt>k']
unmaximize=['<Shift><Alt>j']
toggle-fullscreen=['<Shift><Alt>f']
move-to-monitor-left=['<Super>h']
